{
  "name": "joshies",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild": "node src/setLastUpdatedDate.mjs pre",
    "postbuild": "node src/setLastUpdatedDate.mjs post",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write .",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "generate-db-types": "npx supabase gen types typescript --project-id 'hqomdxggwvkmaovkytld' --schema public > src/app/shared/util/schema.ts"
  },
  "private": true,
  "prettier": {
    "singleQuote": true
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,js,tsx,jsx,css,scss,sass,json,md,yml,yaml,html}": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@angular/service-worker": "^17.1.1",
    "@supabase/supabase-js": "^2.39.2",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.8.0",
    "quill": "^1.3.7",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "~3.1.1",
    "typescript": "~5.2.2"
  }
